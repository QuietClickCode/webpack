{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        [
            "import",
            {
                "libraryName": "ivue-material",
                "libraryDirectory": "src/components"
            }
        ]
    ]
}
